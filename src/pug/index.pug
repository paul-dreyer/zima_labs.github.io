                        div(class="relative max-w-max mx-auto")
                            img(src="images/how_it_works_icons.png", style="max-height: 500px;" alt="")
                        
                        div(class="max-w-lg mx-auto text-center", style="max-width: 600px;")
                            h2(class="font-heading mb-6 text-7xl lg:text-8xl text-green-400 tracking-8xl md:max-w-md mx-auto", style="max-width: 700px;") Synthetic Generation
                            h3(class="mb-6 text-2xl text-white tracking-tighter-xl") Generate synthetic training data from existing CAD assets
                            
                div(class="relative")
                    img(class="absolute top-0 left-48", src="template-assets/images/headers/layer-blur.svg", alt="")
                    img(class="absolute bottom-0 right-0", src="template-assets/images/headers/lines2.svg", alt="")



            section(class="pt-20 pb-24 bg-blueGray-950")
                div(class="container px-4 mx-auto")
                        
                    div(class="flex flex-wrap -m-4")
                    
                        div(class="w-full md:w-1/2 lg:w-1/3 px-4")
                            div(class="relative mb-4 overflow-hidden rounded-5xl group")

                                //- Image
                                img(class="w-full transform group-hover:scale-125 transition duration-1000"
                                    src="template-assets/images/cards/3D_print_purple_large_thumbnail.png", alt="")

                                //- Text overlay pinned to bottom
                                div(class="absolute left-0 bottom-0 w-full p-8 bg-gradient-card flex flex-col justify-end z-10")
                                    h3(class="text-3xl text-white tracking-3xl mb-2") Manufacturing
                                    span(class="text-white text-lg group-hover:underline") See Demo ▶

                                a(class="absolute inset-0 z-20", href="gallery.html")


                        div(class="w-full md:w-1/2 lg:w-1/3 px-4")
                            div(class="relative mb-4 overflow-hidden rounded-5xl group")

                                //- Image
                                img(class="w-full transform group-hover:scale-125 transition duration-1000"
                                    src="template-assets/images/cards/mars_thumbnail_square.png", alt="")

                                //- Text overlay pinned to bottom
                                div(class="absolute left-0 bottom-0 w-full p-8 bg-gradient-card flex flex-col justify-end z-10")
                                    h3(class="text-3xl text-white tracking-3xl mb-2") Robotics
                                    span(class="text-white text-lg group-hover:underline") See Demo ▶

                                //- Transparent click overlay
                                a.see-demo-btn(
                                href="#"
                                data-video-id="NfBHZMWBBbM"
                                class="absolute inset-0 z-20"
                                aria-label="See demo"
                                )
                    
                        div(class="w-full md:w-1/2 lg:w-1/3 px-4")
                            div(class="relative mb-4 overflow-hidden rounded-5xl group")

                                //- Image
                                img(class="w-full transform group-hover:scale-125 transition duration-1000"
                                    src="template-assets/images/cards/wire_harness_thumbnail_square.png", alt="")

                                //- Text overlay pinned to bottom
                                div(class="absolute left-0 bottom-0 w-full p-8 bg-gradient-card flex flex-col justify-end z-10")
                                    h3(class="text-3xl text-white tracking-3xl mb-2") Assembly
                                    span(class="text-white text-lg group-hover:underline") See Demo ▶

                                a(class="absolute inset-0 z-20", href="gallery.html")
                                        
            
            section(class="relative py-18 overflow-hidden")
                div(class="container px-4 mx-auto")

                    div(class="mb-10 md:max-w-2xl text-center mx-auto")
                        //- span(class="inline-block mb-4 text-sm text-green-400 font-medium tracking-tighter") Core Card
                        h2(class="font-heading text-7xl lg:text-8xl text-white tracking-tighter-xl") Data Capabilities

                    
                    div(class="flex flex-wrap -m-5 pb-8")
                        
                        div(class="w-full md:w-1/2 px-4 mt-4")
                            div(class="relative flex flex-col justify-end pb-8 h-full bg-gradient-radial-dark overflow-hidden border border-gray-900 border-opacity-30 rounded-5xl")
                                img(class="mb-8 rounded-5xl", src="template-assets/images/data_types/mars_bounding_box_thumbnail.png", alt="")
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-4xl sm:text-4xl md:text-5xl text-white tracking-tighter break-words text-balance max-w-full") Bounding Box
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-lg text-white tracking-tighter break-words text-balance max-w-full") Locate a box containing the object of interest in the image
                                img(class="absolute bottom-0 right-0", src="template-assets/images/data_types/bg-gray.png", alt="")

                        div(class="w-full md:w-1/2 px-4 mt-4")
                            div(class="relative flex flex-col justify-end pb-8 h-full bg-gradient-radial-dark overflow-hidden border border-gray-900 border-opacity-30 rounded-5xl")
                                img(class="mb-8 rounded-5xl", src="template-assets/images/data_types/contour_thumbnail.png", alt="")
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-4xl sm:text-4xl md:text-5xl text-white tracking-tighter break-words text-balance max-w-full") Edge Contour
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-lg text-white tracking-tighter break-words text-balance max-w-full") A polygon describing the edges of an object of interest
                                img(class="absolute bottom-0 right-0", src="template-assets/images/data_types/bg-gray.png", alt="")

                    
                    div(class="flex flex-wrap -m-5 pb-5")
                        
                        div(class="w-full md:w-1/2 px-4 mt-4")
                            div(class="relative flex flex-col justify-end pb-8 h-full bg-gradient-radial-dark overflow-hidden border border-gray-900 border-opacity-30 rounded-5xl")
                                img(class="mb-8 rounded-5xl", src="template-assets/images/data_types/bins_segmentation_thumbnail.png", alt="")
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-4xl sm:text-4xl md:text-5xl text-white tracking-tighter break-words text-balance max-w-full") Segmentation
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-lg text-white tracking-tighter break-words text-balance max-w-full") Per-pixel masking of an object of interest
                                img(class="absolute bottom-0 right-0", src="template-assets/images/data_types/bg-gray.png", alt="")

                        div(class="w-full md:w-1/2 px-4 mt-4")

                            div(class="relative flex flex-col justify-end pb-8 h-full bg-gradient-radial-dark overflow-hidden border border-gray-900 border-opacity-30 rounded-5xl")
                                img(class="mb-8 rounded-5xl", src="template-assets/images/data_gallery/wire_harness/set_b_pose_overlay.png", alt="")
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-4xl sm:text-4xl md:text-5xl text-white tracking-tighter break-words text-balance max-w-full") Pose
                                div(class="relative z-10 max-w-md text-center mx-auto")
                                    h2(class="text-lg text-white tracking-tighter break-words text-balance max-w-full") The position and orientation of an object of interest
                                img(class="absolute bottom-0 right-0", src="template-assets/images/data_types/bg-gray.png", alt="")
                        
            
            
            //- Features
            section(class="relative py-24 overflow-hidden")

                div(class="mb-10 md:max-w-2xl text-center mx-auto")
                        h2(class="font-heading text-7xl lg:text-8xl text-white tracking-tighter-xl") Features

                div(class="container px-4 mx-auto")
                    
                    //- (side by side for large screens, stacked for small screens)
                    div(class="flex flex-wrap justify-between px-5 pt-5 pb-16 bg-gradient-radial-dark overflow-hidden border border-gray-900 border-opacity-30 rounded-5xl")
                    
                        //- First group
                        div(class="w-full md:w-1/2 mb-14")
                            div(class="relative z-10 md:max-w-xl mx-auto")
                                div(class="flex flex-col items-center mb-6")
                                    h2(class="font-heading text-5xl text-white tracking-tighter-xl") Meta Data
                                
                                h3(class="mb-6 text-xl text-white tracking-tighter-xl") A huge advantage to using synthetic data is the meta-data we can track for each image, allowing for a much deeper understanding of the context of the image. With our data sets, your team can easily create curriculum-learning training routines, only exposing the network to instances with low object occlusion rates early in training, and gradually exposing instances with more object occlusion as the network learns
                                div(class="flex flex-wrap justify-center")
                                    ul
                                        li(class="flex mb-3.5")
                                            img(class="mr-3.5", src="template-assets/images/data_types/checked.svg", alt="")
                                            span(class="text-green-300 text-xl") Object-to-camera occlusion %
                                        li(class="flex mb-3.5")
                                            img(class="mr-3.5", src="template-assets/images/data_types/checked.svg", alt="")
                                            span(class="text-green-300 text-xl") Camera pose (location + orientation) 
                                        li(class="flex mb-3.5")
                                            img(class="mr-3.5", src="template-assets/images/data_types/checked.svg", alt="")
                                            span(class="text-green-300 text-xl") Lighting locations and intensities
                                        li(class="flex")
                                            img(class="mr-3.5", src="template-assets/images/data_types/checked.svg", alt="")
                                            span(class="text-green-300 text-xl") Surface materials and texture tracking 

                        //- Second group
                        div(class="w-full md:w-1/2")
                            div(class="relative z-10 md:max-w-xl mx-auto")
                                div(class="flex flex-col items-center mb-6")
                                    h2(class="font-heading text-5xl text-white tracking-tighter-xl") Python Support
                                
                                h3(class="mb-6 text-xl text-white tracking-tighter-xl") We're building a public python module which offers dataset manipulation utilities, allowing your team to start training sooner and deploy models faster.
                                
                                div(class="flex justify-center mx-auto")
                                    ul
                                        li(class="flex mb-3.5")
                                            img(class="mr-3.5", src="template-assets/images/data_types/checked.svg", alt="")
                                            span(class="text-green-300 text-xl") Domain randomization analysis
                                        li(class="flex mb-3.5")
                                            img(class="mr-3.5", src="template-assets/images/data_types/checked.svg", alt="")
                                            span(class="text-green-300 text-xl") Ablation subset filtering
                                        li(class="flex")
                                            img(class="mr-3.5", src="template-assets/images/data_types/checked.svg", alt="")
                                            span(class="text-green-300 text-xl") Formatting for YOLO training

                                div(class="flex justify-center items-center pt-5")
                                    a(class="inline-flex items-center gap-3 px-8 py-4 text-xl text-white hover:text-black tracking-tighter hover:bg-green-400 border-2 border-white focus:border-green-400 focus:border-opacity-40 hover:border-green-400 focus:ring-4 focus:ring-green-400 focus:ring-opacity-40 rounded-full transition duration-300", href="documentation.html")
                                        img(class="h-8 w-auto", src="template-assets/images/logo-clouds/python-logo.png", alt="Python Logo")
                                        | Read The Docs

                                //- div(class="w-auto hidden lg:block")
                                //-     div(class="inline-block")
                                //-         a(class="inline-block px-8 py-4 text-white hover:text-black tracking-tighter hover:bg-green-400 border-2 border-white focus:border-green-400 focus:border-opacity-40 hover:border-green-400 focus:ring-4 focus:ring-green-400 focus:ring-opacity-40 rounded-full transition duration-300", href="contact.html") Contact
            
            //- Page bottom
            include components/_page_bottom.pug


        //- Modal
        div#video-modal(class="fixed inset-0 bg-black bg-opacity-80 flex items-center justify-center z-50 hidden")
            div(class="relative w-full max-w-4xl aspect-video")
                button#close-modal(class="absolute top-2 right-2 text-white text-3xl font-bold z-10 hover:text-gray-300") &times;
                iframe#video-frame(
                class="w-full h-full rounded-lg"
                src=""
                frameborder="0"
                allow="autoplay; encrypted-media"
                allowfullscreen
                )

script.
  const modal = document.getElementById("video-modal");
  const iframe = document.getElementById("video-frame");
  const closeBtn = document.getElementById("close-modal");

  document.querySelectorAll(".see-demo-btn").forEach((btn) => {
    btn.addEventListener("click", (e) => {
      e.preventDefault();
      const videoId = btn.dataset.videoId;
      iframe.src = `https://www.youtube.com/embed/${videoId}?autoplay=1`;
      modal.classList.remove("hidden");
    });
  });

  closeBtn.addEventListener("click", () => {
    modal.classList.add("hidden");
    iframe.src = "";
  });

  window.addEventListener("keydown", (e) => {
    if (e.key === "Escape") {
      modal.classList.add("hidden");
      iframe.src = "";
    }
  });